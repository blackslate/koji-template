{
  "text": {
    "titleFont": "https://fonts.googleapis.com/css?family=Luckiest+Guy",
    "textFont": "https://fonts.googleapis.com/css?family=Chilanka",

    "buttonTextRatio": 11,
    "buttonHeight": 9,
    "buttonBorder": 1,
    "buttonRadius": 2,

    "headerHeight": 8,
    "headerFontSize": 3.2,

    "tooltipFontSize": 1.2,
    "tooltipTextColor": "#000",
    "tooltipBgColor": "#fe9",
    "tooltipEdgeColor": "#000",
    "tooltipBorder": 0.1,
    "tooltipRadius": 1
  },
  "@@editor": [{
    "key": "text",
    "name": "Fonts & Sizes",
    "icon": "‚úç",
    "source": "text.json",
    "fields": [{
        "key": "titleFont",
        "name": "Text Font Family",
        "description": "Google font to use for titles and buttons",
        "type": "text"
      },
      {
        "key": "textFont",
        "name": "Text Font Family",
        "description": "Google fonts link to font to use for text",
        "type": "text"
      },
      {
        "key": "buttonTextRatio",
        "name": "Button Text Ratio",
        "description": "Relative font size for buttons",
        "type": "range",
        "typeOptions": {
          "min": 2,
          "max": 20,
          "step": 0.1
        }
      },
      {
        "key": "buttonHeight",
        "name": "Button Height",
        "description": "Relative height for buttons",
        "type": "range",
        "typeOptions": {
          "min": 2,
          "max": 20,
          "step": 0.1
        }
      },
      {
        "key": "buttonBorder",
        "name": "Button Border",
        "description": "Border width for buttons",
        "type": "range",
        "typeOptions": {
          "min": 0,
          "max": 5,
          "step": 0.1
        }
      },
      {
        "key": "buttonRadius",
        "name": "Button Corner Radius",
        "description": "Corner radius for buttons",
        "type": "range",
        "typeOptions": {
          "min": 0,
          "max": 50,
          "step": 0.1
        }
      },
      {
        "key": "headerHeight",
        "name": "Header Height",
        "description": "Height of headers",
        "type": "range",
        "typeOptions": {
          "min": 2,
          "max": 20,
          "step": 0.1
        }
      },
      {
        "key": "headerFontSize",
        "name": "Header Font Size",
        "description": "Font size of headers",
        "type": "range",
        "typeOptions": {
          "min": 2,
          "max": 20,
          "step": 0.1
        }
      },
      {
        "key": "tooltipFontSize",
        "name": "Tooltip Font Size",
        "description": "Font size of tooltips",
        "type": "range",
        "typeOptions": {
          "min": 0.5,
          "max": 5,
          "step": 0.1
        }
      },
      {
        "key": "tooltipTextColor",
        "name": "Tooltip Text Color",
        "type": "color"
      },
      {
        "key": "tooltipBgColor",
        "name": "Tooltip Background Color",
        "type": "color"
      },
      {
        "key": "tooltipEdgeColor",
        "name": "Tooltip Border Color",
        "description": "Color of tooltip borders",
        "type": "color"
      },
      {
        "key": "tooltipBorder",
        "name": "Tooltip Border Width",
        "description": "Thickness of tooltip borders",
        "type": "range",
        "typeOptions": {
          "min": 0,
          "max": 5,
          "step": 0.1
        }
      },
      {
        "key": "tooltipRadius",
        "name": "Tooltip Corner Radius",
        "description": "Radius of tooltip corners",
        "type": "range",
        "typeOptions": {
          "min": 0,
          "max": 20,
          "step": 0.1
        }
      }
    ]
  }]
}